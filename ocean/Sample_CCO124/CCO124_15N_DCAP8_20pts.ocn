converge( 'ic "/I2<7>/net9" "1" )
converge( 'ic "/I2<8>/net11" "0" )
converge( 'ic "/I2<8>/net9" "1" )
converge( 'ic "/I2<9>/net11" "0" )
converge( 'ic "/I2<9>/net9" "1" )
converge( 'ic "/I2<10>/net11" "0" )
converge( 'ic "/I2<10>/net9" "1" )
converge( 'ic "/I2<11>/net11" "0" )
converge( 'ic "/I2<11>/net9" "1" )
converge( 'ic "/I2<12>/net11" "0" )
converge( 'ic "/I2<12>/net9" "1" )
converge( 'ic "/I2<13>/net11" "0" )
converge( 'ic "/I2<13>/net9" "1" )
converge( 'ic "/I2<14>/net11" "0" )
converge( 'ic "/I2<14>/net9" "1" )
converge( 'ic "/I2<15>/net11" "0" )
converge( 'ic "/I2<15>/net9" "1" )
		analysis('tran ?stop "8u"  ?errpreset "conservative"  )
		save( 'v "/X<1>" )
                save('v "/VDD")
		run()
		
		;vctrl = (average(v("/X<1>" ?result "tran")) * 2)
                vctrl = average(v("/VDD" ?result "tran"))
		R = vctrl/IDC
		

		
		delay = delay(?wf1 v("/X<1>" ?result "tran") ?value1 0.15 ?edge1 "rising" ?nth1 8 ?wf2 v("/X<1>" ?result "tran") ?value2 0.15 ?edge2 "rising" ?nth2 9)

			
		fprintf(out "%1.5e \t" delay)
		fprintf(out "%1.5e \t" vctrl)
		fprintf(out "%1.5e \t" R)		

	)
	fprintf(out "\n")

)

