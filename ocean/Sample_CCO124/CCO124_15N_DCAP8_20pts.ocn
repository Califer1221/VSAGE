temp( 27 ) 
selectResult( 'tran )


out=outfile("./ocean/DCAP/20pts/result/CCO124_20pts/Delay_CCO_ring124_15N_DCAP8_20pts_sample.txt" "w")

foreach(IDC IDCLIST
	foreach( CLOAD CLOADLIST
	
		delay=0
		desVar( "idc" IDC )
		desVar( "cload" CLOAD)
		converge( 'ic "/X<1>" "0" )
        	converge( 'ic "/XN<1>" "1" )
converge( 'ic "/I2<1>/net9" "1" )
converge( 'ic "/I2<2>/net11" "0" )
converge( 'ic "/I2<2>/net9" "1" )
converge( 'ic "/I2<3>/net11" "0" )
converge( 'ic "/I2<3>/net9" "1" )
